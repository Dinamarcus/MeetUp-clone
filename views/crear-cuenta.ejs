<main class="contenedor contenedor-formularios">
    <h1><%= nombrePagina %></h1>

    <form action="/crear-cuenta" method="POST" class="default-form" autocomplete="off">
        <div class="campo">
            <label for="email">E-mail</label>
            <input type="email" name="email" placeholder="Tu Email" value="<%= email %>">
        </div>
        <div class="campo">
            <label for="nombre">Nombre</label>
            <input type="text" name="nombre" placeholder="Tu Nombre" value="<%= nombre %>">
        </div>
        <div class="campo">
            <label for="password">Password</label>
            <input type="password" name="password" placeholder="Tu Password">
        </div>
        <div class="campo">
            <label for="repetir">Repetir Password</label>
            <input type="password" name="repetir" placeholder="Repite tu password">
        </div>
        <div class="campo enviar">
            <input type="submit" value="Crear Cuenta" class="btn btn-rosa">
        </div>
    </form>
</main>